{
	"name": "cross-validator",
	"version": "0.0.1-snapshot",
	"description": "A Cross Script Validator, node, browser, java",
	"main": "src/index.js",
	"scripts": {
		"test": "./node_modules/mocha/bin/mocha test/*.js test/*/*.js test/*/*/*.js",
		"watch": "./node_modules/mocha/bin/mocha --watch test/*.js test/*/*.js test/*/*/*.js",
		"jshint": "./node_modules/.bin/jshint ./src"
	},
	"keywords": [
		"cross",
		"validate",
		"validator",
		"validating",
		"javascript",
		"java",
		"node"
	],
	"author": "Guilherme M Gregio <guilherme@gregio.net>",
	"license": "MIT",
	"devDependencies": {
		"chai": "^1.9.1",
		"jshint": "^2.5.3",
		"mocha": "^1.21.4"
	}
}
